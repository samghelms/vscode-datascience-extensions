{
	"name": "vscode-jupyterfs",
	"displayName": "Jupyter file system",
	"description": "Using Jupyter to implement a file system for vscode",
	"version": "0.0.1",
	"publisher": "samghelms",
	"repository": {
		"url": "",
		"type": "git"
	},
	"engines": {
		"vscode": "^1.23.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onFileSystem:jupyterfs",
		"onCommand:jupyterfs.workspaceInit"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "jupyterfs.workspaceInit",
				"title": "Setup Jupyter Workspace",
				"category": "JupyterFS"
			},
			{
				"command": "jupyterfs.connectJupyter",
				"title": "Connect to a Jupyter Server",
				"category": "JupyterFS"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "jupyterfs.workspaceInit",
					"when":  "workbenchState != workspace"
				},
				{
					"command": "jupyterfs.connectJupyter",
					"when":  "workbenchState == workspace"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/node": "^7.0.43",
		"tslint": "^5.11.0",
		"typescript": "^2.5.2",
		"vscode": "^1.1.17"
	},
	"dependencies": {
		"@jupyterlab/services": "^3.2.1",
		"node-fetch": "^2.3.0",
		"url-parse": "^1.4.4",
		"vscode-helpers": "^3.0.0",
		"ws": "^6.1.2"
	}
}
