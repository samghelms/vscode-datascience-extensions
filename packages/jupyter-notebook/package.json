{
	"name": "jupyter-notebook-vscode",
	"description": "Runs a jupyter notebook in vscode",
	"version": "0.0.1",
	"publisher": "vscode-samples",
	"engines": {
		"vscode": "^1.25.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onWebviewPanel:ipythonView",
		"onLanguage:ipython",
		"onCommand:ipython.showPreview"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
			  "id": "ipython",
			  "aliases": [
				"Jupyter",
				"jupyter",
				"ipython"
			  ],
			  "extensions": [
				".ipynb"
			  ]
			}
		],
		"commands": [
			{
				"command": "ipython.showPreview",
				"title": "Open interactive jupyter session for notebook",
				"category": "Ipython",
				"icon": {
					"light": "./media/launcher-light.svg",
					"dark": "./media/launcher-dark.svg"
				}
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "ipython.showPreview",
					"when": "editorLangId == ipython",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -w -p ./",
		"watch-notebook": "tsc -w -p ./src/notebook",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"dependencies": {
		"@jupyterlab/services": "^3.2.1",
		"node-fetch": "^2.3.0",
		"vscode": "^1.1.18",
		"vscode-helpers": "^3.0.0"
	},
	"devDependencies": {
		"@types/node": "^10.5.2",
		"ts-loader": "^5.3.3",
		"tslint": "^5.11.0",
		"typescript": "^2.9.2",
		"webpack-dev-server": "^3.1.14"
	}
}
